          },
          "pendingApplication": {
            "type": "boolean"
          },
          "allowMultipleValues": {
            "type": "boolean"
          },
          "applyWithoutRestart": {
            "type": "boolean"
          },
          "permittedRangeStart": {
            "type": "integer",
            "format": "int32"
